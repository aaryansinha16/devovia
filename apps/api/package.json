{
  "name": "api",
  "version": "0.0.1",
  "description": "express api application",
  "scripts": {
    "prebuild": "rimraf dist && node -e \"const { schemaPath } = require('./prisma/prisma-schema-path.js'); process.env.PRISMA_SCHEMA_PATH = schemaPath; require('child_process').execSync('npx prisma generate --schema=' + schemaPath, {stdio: 'inherit'})\"",
    "build": "tsc",
    "build:railway": "rimraf dist && RAILWAY_ENVIRONMENT=true node -e \"const { schemaPath } = require('./prisma/prisma-schema-path.js'); process.env.PRISMA_SCHEMA_PATH = schemaPath; require('child_process').execSync('npx prisma generate --schema=' + schemaPath, {stdio: 'inherit'})\" && tsc -p tsconfig.json",
    "start:railway": "node dist/railway.js",
    "prisma:deploy": "prisma migrate deploy",
    "postinstall": "echo 'Skipping prisma generate in postinstall'",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/index.js",
    "start:dev": "nodemon --exec ts-node src/index.ts",
    "start:debug": "nodemon --exec node --inspect-brk -r ts-node/register src/index.ts",
    "start:prod": "node dist/index.js",
    "dev": "tsc --watch --preserveWatchOutput & node --watch dist/index.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "check-types": "tsc --noEmit",
    "test": "node --test src/**/__tests__/**/*.test.js",
    "test:auth": "node --test src/controllers/__tests__/auth.controller.test.js",
    "test:oauth": "node --test src/controllers/__tests__/oauth.controller.test.js",
    "test:watch": "node --test --watch src/**/__tests__/**/*.test.js",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "vercel-build": "cd ../../packages/database && npx prisma generate && cd ../../apps/api && tsc"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "@repo/database": "file:../../packages/database",
    "@types/cookie-parser": "^1.4.8",
    "@types/multer": "^1.4.12",
    "@vercel/node": "^3.0.17",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.6.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.11.25",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsdk/create-nestjs-app.git",
    "directory": "packages/nest-myapp-template"
  },
  "keywords": [
    "nestjs",
    "typescript"
  ],
  "author": "zaczheng <st_zzh@sina.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsdk/create-nestjs-app/issues"
  },
  "homepage": "https://github.com/tsdk/create-nestjs-app",
  "gitHead": "ac9072902aff8d26df5ab02f85f25af7d1b85bb8"
}
